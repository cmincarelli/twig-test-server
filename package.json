    "url": "git+https://github.com/cmincarelli/twig-test-server.git"
  },
  "author": "Christopher Mincarelli",
  "license": "Apache",
  "dependencies": {
    "cheerio": "^0.19.0",
    "express": "^4.13.3",
    "twig": "^0.8.2",
    "yargs": "^3.31.0"
  },
  "homepage": "https://github.com/cmincarelli/twig-test-server#readme",
  "files": [
    "bin",
    "lib",
    "data",
    "config"
